{
	"name": "@gmaps-tools/pbfish",
	"version": "1.2.0",
	"description": "A parser for Google Maps' protobuf-like formats",
	"author": "Emily Nagy",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.23.9",
		"@babel/preset-env": "^7.23.9",
		"@babel/preset-typescript": "^7.23.3",
		"@types/jest": "^29.5.2",
		"babel-jest": "^29.7.0",
		"babel-plugin-remove-import-js-extension": "^0.0.3",
		"jest": "^29.5.0",
		"typescript": "^4.9.5"
	},
	"scripts": {
		"test": "jest",
		"cov": "jest --coverage",
		"build": "rm -rf dist && npx tsc -p tsconfig-es6.json && npx tsc -p tsconfig-cjs.json && cp dist/cjs/index.d.ts dist/index.d.ts"
	},
	"main": "./dist/cjs/index.js",
	"module": "./dist/es6/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"require": "./dist/cjs/index.js",
		"import": "./dist/es6/index.js"
	}
